{:paths ["src" "resources"]
 :deps {org.clojure/clojure {:mvn/version "1.12.0"}
        org.clojure/clojurescript {:mvn/version "1.11.132"}
        quil/quil {:mvn/version "4.3.1563"}
        speclj/speclj {:mvn/version "3.10.0"}}
 :aliases {:test {:extra-paths ["spec"]
                   :main-opts ["-m" "speclj.main" "-c"]}
					 :spec  {:main-opts ["-m" "speclj.main" "-c"]
                   :extra-deps  {speclj/speclj {:mvn/version "3.10.0"}}
                   :extra-paths ["spec"]}
	         :empire {:main-opts [-m empire.ui.core]}
					 :run {:main-opts [-m empire.ui.core]}
					 :cljs {:main-opts ["-m" "cljs.main"
					                         "--optimizations" "none"
					                         "--compile" "empire.ui.core"
																	 ]}
					 :cov {:main-opts ["-m" "speclj.cloverage" "--" "-p" "src" "-s" "spec"]
					                  :extra-deps {cloverage/cloverage {:mvn/version "1.2.4"}}
					                  :extra-paths ["spec"]}
					 :parse-tests {:main-opts ["-m" "empire.acceptance.parser"]
					               :extra-paths ["spec"]}
					 :generate-specs {:main-opts ["-m" "empire.acceptance.generator"]
					                  :extra-paths ["generated-acceptance-specs"]}
				 }
			 }
